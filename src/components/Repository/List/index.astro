---
import { Repository } from '../type';

interface Props {
  repositories: Repository[];
}

const { repositories } = Astro.props;
---

<ul>
  {
    repositories.map((x) => {
      const { repositoryName, repositoryUrl, languagesUrl } = x;
      return (
        <li class="item">
          <span>{repositoryName}</span>
        </li>
      );
    })
  }
</ul>

<style>
  .item {
    padding: 10px 0 10px 0;
    color: #ffffff;
    border-radius: 10px;
    cursor: pointer;
  }
</style>
